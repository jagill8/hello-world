[project]
	description = Introduction to Github using Hello World 
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/jagill8
[access "refs/for/refs/*"]
	read = group user/jagill8
	push = group user/jagill8
	label-Code-Review = -2..+2 group user/jagill8
	label-Verified = -1..+1 group user/jagill8
	rebase = group user/jagill8
	removeReviewer = group user/jagill8
	submit = group user/jagill8
[access "refs/heads/*"]
	read = group user/jagill8
	create = group user/jagill8
	push = +force group user/jagill8
	pushMerge = group user/jagill8
[access "refs/tags/*"]
	push = group user/jagill8
	push = +force group user/jagill8
